<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape >
            <gradient
                android:startColor="#ccbc4c6a"
                android:endColor="#ccd96565"
                android:type="linear"
                android:angle="180"/>
            <corners android:radius="50dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient
                android:startColor="#ccec5d83"
                android:endColor="#ccff7575"
                android:type="linear"
                android:angle="180"/>
            <corners android:radius="50dp"/>
        </shape>
    </item>
</selector>