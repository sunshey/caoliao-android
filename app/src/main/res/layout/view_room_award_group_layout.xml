<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:id="@+id/view_award_group">
        <com.yc.liaolive.gift.view.RoomAwardItemView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:id="@+id/view_award_child1"
            android:clipChildren="false"
            android:clipToPadding="false" />
        <com.yc.liaolive.gift.view.RoomAwardItemView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:id="@+id/view_award_child2"
            android:clipChildren="false"
            android:clipToPadding="false"/>
        <com.yc.liaolive.gift.view.RoomAwardItemView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:id="@+id/view_award_child3"
            android:clipChildren="false"
            android:clipToPadding="false"/>
    </LinearLayout>
    <com.yc.liaolive.gift.manager.RoomSuperAwardAnimatorGroupManager
        android:id="@+id/award_animator_min_manager"
        android:layout_width="158dp"
        android:layout_height="match_parent"/>
</FrameLayout>